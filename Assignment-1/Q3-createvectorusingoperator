//wrt a function  that creates a vector of a user given size M using new Operator
#include<iostream>
using namespace std;
int*crVector(int m){
    int* arr = new int[m];
    return arr;
}
int main(){
    int m;
    cout<<"Enter the value of m :";
    cin >> m;
    int*vec = crVector(m);
    for(int i = 0 ; i < m ; i++){
        vec[i] = i + 1;
        cout << vec[i] << " ";
    }
    cout << endl;
    delete[] vec;
    return 0;
}
